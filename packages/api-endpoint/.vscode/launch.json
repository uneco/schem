{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Node Inspector",
			"type": "node",
			"request": "launch",
			"args": ["${workspaceRoot}/src/index.ts"],
			"runtimeArgs": ["-r", "ts-node/register/transpile-only"],
			"cwd": "${workspaceRoot}",
			"protocol": "inspector",
			"internalConsoleOptions": "openOnSessionStart",
			"env": {
				"TS_NODE_IGNORE": "false"
			}
		}
	]
}
